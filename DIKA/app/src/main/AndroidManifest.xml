<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wisteg">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/tegal"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tegal"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.wisteg.DaftarWisata"></activity>
        <activity android:name="com.example.wisteg.Detail" />
        <activity android:name="com.example.wisteg.Home" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name="com.example.wisteg.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.wisteg.Map" />
    </application>

</manifest>